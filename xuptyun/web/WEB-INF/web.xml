<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.xupt.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>announcementServlet</servlet-name>
        <servlet-class>com.xupt.servlet.AnnouncementServlet.AnnouncementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>announcementServlet</servlet-name>
        <url-pattern>/announcementServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>AddAnnouncementServlet</servlet-name>
        <servlet-class>com.xupt.servlet.AnnouncementServlet.AddAnnouncementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddAnnouncementServlet</servlet-name>
        <url-pattern>/AddAnnouncementServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>EditAnnouncementServlet</servlet-name>
        <servlet-class>com.xupt.servlet.AnnouncementServlet.EditAnnouncementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditAnnouncementServlet</servlet-name>
        <url-pattern>/EditAnnouncementServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>UpdateAnnouncementServlet</servlet-name>
        <servlet-class>com.xupt.servlet.AnnouncementServlet.UpdateAnnouncementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateAnnouncementServlet</servlet-name>
        <url-pattern>/UpdateAnnouncementServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DeleteAnnouncementServlet</servlet-name>
        <servlet-class>com.xupt.servlet.AnnouncementServlet.DeleteAnnouncementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteAnnouncementServlet</servlet-name>
        <url-pattern>/DeleteAnnouncementServlet</url-pattern>
    </servlet-mapping>
</web-app>